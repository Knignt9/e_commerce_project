<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>RegistServlet</servlet-name>
    <servlet-class>cn.tedu.web.RegistServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ValiImageServlet</servlet-name>
    <servlet-class>cn.tedu.web.ValiImageServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjaxCheckUsernameServlet</servlet-name>
    <servlet-class>cn.tedu.web.AjaxCheckUsernameServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>cn.tedu.web.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>cn.tedu.web.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageAddProdServlet</servlet-name>
    <servlet-class>cn.tedu.web.backend.ManageAddProdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProdListServlet</servlet-name>
    <servlet-class>cn.tedu.web.ProdListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProdImgServlet</servlet-name>
    <servlet-class>cn.tedu.web.ProdImgServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageDelProdServlet</servlet-name>
    <servlet-class>cn.tedu.web.backend.ManageDelProdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageProdListServlet</servlet-name>
    <servlet-class>cn.tedu.web.backend.ManageProdListServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjaxChangePnumServlet</servlet-name>
    <servlet-class>cn.tedu.web.backend.AjaxChangePnumServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AjaxProdInfoServlet</servlet-name>
    <servlet-class>cn.tedu.web.AjaxProdInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddCartServlet</servlet-name>
    <servlet-class>cn.tedu.web.AddCartServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DelCartProdServlet</servlet-name>
    <servlet-class>cn.tedu.web.DelCartProdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManageCartProdServlet</servlet-name>
    <servlet-class>cn.tedu.web.ManageCartProdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckProdServlet</servlet-name>
    <servlet-class>cn.tedu.web.CheckProdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PhoneListServlet</servlet-name>
    <servlet-class>cn.tedu.web.PhoneListServlet</servlet-class>
  </servlet>














  <context-param>
  	<param-name>encode</param-name>
  	<param-value>utf-8</param-value>
  </context-param>
  <filter>
  	<filter-name>EncodingFilter</filter-name>
  	<filter-class>cn.tedu.filter.EncodingFilter</filter-class>
  </filter>
  <filter>
  	<filter-name>AutoLoginFilter</filter-name>
  	<filter-class>cn.tedu.filter.AutoLoginFilter</filter-class>
  </filter>




  <servlet-mapping>
    <servlet-name>RegistServlet</servlet-name>
    <url-pattern>/servlet/RegistServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ValiImageServlet</servlet-name>
    <url-pattern>/servlet/ValiImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjaxCheckUsernameServlet</servlet-name>
    <url-pattern>/servlet/AjaxCheckUsernameServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/servlet/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageAddProdServlet</servlet-name>
    <url-pattern>/servlet/ManageAddProdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProdListServlet</servlet-name>
    <url-pattern>/servlet/ProdListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProdImgServlet</servlet-name>
    <url-pattern>/servlet/ProdImgServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageDelProdServlet</servlet-name>
    <url-pattern>/servlet/ManageDelProdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageProdListServlet</servlet-name>
    <url-pattern>/servlet/ManageProdListServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjaxChangePnumServlet</servlet-name>
    <url-pattern>/servlet/AjaxChangePnumServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AjaxProdInfoServlet</servlet-name>
    <url-pattern>/servlet/AjaxProdInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddCartServlet</servlet-name>
    <url-pattern>/servlet/AddCartServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DelCartProdServlet</servlet-name>
    <url-pattern>/servlet/DelCartProdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManageCartProdServlet</servlet-name>
    <url-pattern>/servlet/ManageCartProdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckProdServlet</servlet-name>
    <url-pattern>/servlet/CheckProdServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PhoneListServlet</servlet-name>
    <url-pattern>/servlet/PhoneListServlet</url-pattern>
  </servlet-mapping>
	
  <filter-mapping>
  	<filter-name>EncodingFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter-mapping>
  	<filter-name>AutoLoginFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
